                                    <Image Source="/AIStudio.Wpf.DemoPage;component/Resources/Img/DevOps/DevOps-Overview.png" Width="24" Height="24"/>
                                </util:SideMenuItem.Icon>
                                <util:SideMenuItem Header="Summary" Command="{Binding SelectCmd}" CommandParameter="{Binding Header,RelativeSource={RelativeSource Self}}">
                                    <util:SideMenuItem.Icon>
                                        <TextBlock Text="&#xf2cb;" Style="{StaticResource TextBlockFabricIcons}"/>
                                    </util:SideMenuItem.Icon>
                                </util:SideMenuItem>
                                <util:SideMenuItem Header="Dashboards" Command="{Binding SelectCmd}" CommandParameter="{Binding Header,RelativeSource={RelativeSource Self}}">
