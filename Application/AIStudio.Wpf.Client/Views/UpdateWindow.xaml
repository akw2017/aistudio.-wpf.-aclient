<util:WindowBase x:Class="AIStudio.Wpf.Client.Views.UpdateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AIStudio.Wpf.Client.Views"
        xmlns:util="clr-namespace:Util.Controls;assembly=Util.Controls"    
        Style="{StaticResource BaseWindowStyle}"
        mc:Ignorable="d"
        Title="升级" Height="288" Width="450">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" TextElement.FontSize="20">
            <TextBlock x:Name="CurrentVersion" Text="Loading..."/>
            <util:BusyIndicator IsBusy="True" Content="正在检查更新..." />
        </StackPanel>
    </Grid>
</util:WindowBase>
