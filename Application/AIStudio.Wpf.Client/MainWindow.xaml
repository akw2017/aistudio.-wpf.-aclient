<util:WindowBase x:Class="AIStudio.Wpf.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:local="clr-namespace:AIStudio.Wpf.Client"
        xmlns:util="clr-namespace:Util.Controls;assembly=Util.Controls" 
        xmlns:core="clr-namespace:AIStudio.Core;assembly=AIStudio.Core"
        Style="{StaticResource BaseWindowStyle}"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Identifier="{Binding Identifier}"
        CaptionForeground ="{DynamicResource TitleForegroundBrush}"
        CaptionHeight="28"
        mc:Ignorable="d"
        Title="{x:Static core:LocalSetting.Title}" Icon="A.ico" Height="450" Width="800">  
    <Grid>
        <ContentControl  prism:RegionManager.RegionName="{x:Static core:RegionName.MainContentRegion}" />
    </Grid>
</util:WindowBase>
