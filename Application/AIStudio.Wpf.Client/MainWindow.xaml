<util:WindowBase
    x:Class="AIStudio.Wpf.Client.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:core="clr-namespace:AIStudio.Core;assembly=AIStudio.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AIStudio.Wpf.Client"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:util="clr-namespace:Util.Controls;assembly=Util.Controls"
    Title="{x:Static core:LocalSetting.Title}"
    Width="1060"
    Height="800"
    prism:ViewModelLocator.AutoWireViewModel="True"
    CaptionForeground="{DynamicResource TitleForegroundBrush}"
    CaptionHeight="36"
    Icon="A.ico"
    Topmost="True"
    Identifier="{Binding Identifier}"
    Style="{StaticResource BaseWindowStyle}"
    mc:Ignorable="d">
    <Grid WindowChrome.IsHitTestVisibleInChrome="True">
        <ContentControl prism:RegionManager.RegionName="{x:Static core:RegionName.MainContentRegion}" />
    </Grid>
</util:WindowBase>
